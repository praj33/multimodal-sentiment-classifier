      "source": [
        "## 🤖 Uniguru Sentiment Agent Implementation\n",
        "\n",
        "Here's the core implementation of the Uniguru Sentiment Agent optimized for Colab."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "agent_implementation"
      },
      "outputs": [],
      "source": [
        "# Simplified Uniguru Agent for Colab\n",
        "class ColabUniguruAgent:\n",
        "    \"\"\"Simplified Uniguru Sentiment Agent for Google Colab\"\"\"\n",
        "    \n",
        "    def __init__(self):\n",
        "        self.persona_configs = {\n",
        "            \"youth\": {\n",
        "                \"tone_mapping\": {\n",
        "                    \"positive\": [\"enthusiastic\", \"excited\", \"confident\", \"motivated\"],\n",
        "                    \"negative\": [\"frustrated\", \"disappointed\", \"stressed\", \"overwhelmed\"],\n",
        "                    \"neutral\": [\"focused\", \"contemplative\", \"steady\", \"balanced\"]\n",
        "                },\n",
        "                \"tts_emotions\": {\"positive\": \"energetic\", \"negative\": \"concerned\", \"neutral\": \"calm\"}\n",
        "            },\n",
        "            \"kids\": {\n",
        "                \"tone_mapping\": {\n",
        "                    \"positive\": [\"joyful\", \"playful\", \"happy\", \"cheerful\"],\n",
        "                    \"negative\": [\"sad\", \"confused\", \"worried\", \"upset\"],\n",
        "                    \"neutral\": [\"curious\", \"thoughtful\", \"peaceful\", \"content\"]\n",
        "                },\n",
        "                \"tts_emotions\": {\"positive\": \"cheerful\", \"negative\": \"gentle\", \"neutral\": \"friendly\"}\n",
        "            },\n",
        "            \"spiritual\": {\n",
        "                \"tone_mapping\": {\n",
        "                    \"positive\": [\"serene\", \"enlightened\", \"peaceful\", \"grateful\"],\n",
        "                    \"negative\": [\"troubled\", \"seeking\", \"reflective\", \"contemplative\"],\n",
        "                    \"neutral\": [\"meditative\", \"centered\", \"mindful\", \"balanced\"]\n",
        "                },\n",
        "                \"tts_emotions\": {\"positive\": \"serene\", \"negative\": \"compassionate\", \"neutral\": \"peaceful\"}\n",
        "            }\n",
        "        }\n",
        "        \n",
        "        # Initialize basic sentiment analyzer\n",
        "        try:\n",
        "            from transformers import pipeline\n",
        "            self.sentiment_pipeline = pipeline(\"sentiment-analysis\", \n",
        "                                              model=\"distilbert-base-uncased-finetuned-sst-2-english\")\n",
        "            print(\"✅ Sentiment model loaded successfully!\")\n",
        "        except Exception as e:\n",
        "            print(f\"⚠️ Model loading failed: {e}\")\n",
        "            self.sentiment_pipeline = None\n",
        "    \n",
        "    def detect_language(self, text: str) -> str:\n",
        "        \"\"\"Simple language detection\"\"\"\n",
        "        try:\n",
        "            from langdetect import detect\n",
        "            lang_code = detect(text)\n",
        "            lang_map = {'en': 'English', 'es': 'Spanish', 'fr': 'French', 'de': 'German'}\n",
        "            return lang_map.get(lang_code, lang_code)\n",
        "        except:\n",
        "            return \"English\"\n",
        "    \n",
        "    def analyze_sentiment(self, text: str) -> tuple:\n",
        "        \"\"\"Analyze text sentiment\"\"\"\n",
        "        if not self.sentiment_pipeline:\n",
        "            # Fallback simple analysis\n",
        "            positive_words = ['good', 'great', 'excellent', 'amazing', 'wonderful', 'love', 'like', 'happy', 'joy']\n",
        "            negative_words = ['bad', 'terrible', 'awful', 'hate', 'dislike', 'sad', 'angry', 'frustrated']\n",
        "            \n",
        "            text_lower = text.lower()\n",
        "            pos_count = sum(1 for word in positive_words if word in text_lower)\n",
        "            neg_count = sum(1 for word in negative_words if word in text_lower)\n",
        "            \n",
        "            if pos_count > neg_count:\n",
        "                return \"positive\", 0.7\n",
        "            elif neg_count > pos_count:\n",
        "                return \"negative\", 0.7\n",
        "            else:\n",
        "                return \"neutral\", 0.6\n",
        "        \n",
        "        try:\n",
        "            result = self.sentiment_pipeline(text)[0]\n",
        "            label = result['label'].lower()\n",
        "            confidence = result['score']\n",
        "            \n",
        "            # Map labels\n",
        "            if label in ['positive', 'pos']:\n",
        "                sentiment = 'positive'\n",
        "            elif label in ['negative', 'neg']:\n",
        "                sentiment = 'negative'\n",
        "            else:\n",
        "                sentiment = 'neutral'\n",
        "            \n",
        "            return sentiment, confidence\n",
        "        except Exception as e:\n",
        "            print(f\"Analysis error: {e}\")\n",
        "            return \"neutral\", 0.5\n",
        "    \n",
        "    def get_persona_tone(self, sentiment: str, confidence: float, persona: str) -> str:\n",
        "        \"\"\"Get persona-specific tone\"\"\"\n",
        "        if persona not in self.persona_configs:\n",
        "            persona = \"youth\"\n",
        "        \n",
        "        tones = self.persona_configs[persona][\"tone_mapping\"].get(sentiment, [\"neutral\"])\n",
        "        return tones[0] if confidence > 0.7 else tones[-1]\n",
        "    \n",
        "    def get_tts_emotion(self, sentiment: str, persona: str) -> str:\n",
        "        \"\"\"Get TTS emotion\"\"\"\n",
        "        if persona not in self.persona_configs:\n",
        "            persona = \"youth\"\n",
        "        return self.persona_configs[persona][\"tts_emotions\"].get(sentiment, \"neutral\")\n",
        "    \n",
        "    async def predict(self, json_input: Union[str, Dict[str, Any]]) -> Dict[str, Any]:\n",
        "        \"\"\"Main prediction function\"\"\"\n",
        "        start_time = time.time()\n",
        "        \n",
        "        try:\n",
        "            # Parse input\n",
        "            if isinstance(json_input, str):\n",
        "                input_data = json.loads(json_input)\n",
        "            else:\n",
        "                input_data = json_input\n",
        "            \n",
        "            text = input_data.get(\"text\", \"\").strip()\n",
        "            image_url = input_data.get(\"image_url\", \"\").strip()\n",
        "            persona = input_data.get(\"persona\", \"youth\").lower()\n",
        "            \n",
        "            if not text and not image_url:\n",
        "                return {\"error\": \"No input provided\"}\n",
        "            \n",
        "            # Analyze text\n",
        "            if text:\n",
        "                sentiment, confidence = self.analyze_sentiment(text)\n",
        "                language = self.detect_language(text)\n",
        "            else:\n",
        "                # Placeholder for image analysis\n",
        "                sentiment, confidence = \"neutral\", 0.6\n",
        "                language = None\n",
        "            \n",
        "            # Get persona-specific outputs\n",
        "            tone = self.get_persona_tone(sentiment, confidence, persona)\n",
        "            tts_emotion = self.get_tts_emotion(sentiment, persona)\n",
        "            \n",
        "            processing_time = (time.time() - start_time) * 1000\n",
        "            \n",
        "            result = {\n",
        "                \"sentiment\": sentiment,\n",
        "                \"tone\": tone,\n",
        "                \"confidence\": round(confidence, 3),\n",
        "                \"tts_emotion\": tts_emotion,\n",
        "                \"persona\": persona,\n",
        "                \"processing_time_ms\": round(processing_time, 2),\n",
        "                \"timestamp\": datetime.now().isoformat()\n",
        "            }\n",
        "            \n",
        "            if language:\n",
        "                result[\"language\"] = language\n",
        "            \n",
        "            return result\n",
        "            \n",
        "        except Exception as e:\n",
        "            return {\n",
        "                \"error\": \"Prediction failed\",\n",
        "                \"details\": str(e),\n",
        "                \"timestamp\": datetime.now().isoformat()\n",
        "            }\n",
        "\n",
        "# Initialize the agent\n",
        "agent = ColabUniguruAgent()\n",
        "print(\"🎓 Uniguru Sentiment Agent ready for Colab!\")"
      ]
    },
